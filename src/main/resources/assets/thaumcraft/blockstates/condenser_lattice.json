{    
    "multipart": [
        {   "when": { "north": true },
            "apply": { "model": "thaumcraft:condenser_lattice_side", "y": 180, "x": 90 }
        },        
        {   "when": { "south": true },
            "apply": { "model": "thaumcraft:condenser_lattice_side", "x": 90 }
        },
        {   "when": { "east": true },
            "apply": { "model": "thaumcraft:condenser_lattice_side", "y": 270, "x": 90 }
        },
        {   "when": { "west": true },
            "apply": { "model": "thaumcraft:condenser_lattice_side", "y": 90, "x": 90 }
        },
        {   "when": { "up": true },
            "apply": { "model": "thaumcraft:condenser_lattice_side", "x": 180 }
        },
        {   "when": { "down": true },
            "apply": { "model": "thaumcraft:condenser_lattice_side" }
        },
        {   
            "apply": { "model": "thaumcraft:condenser_lattice_core" }
        }
    ]
}
